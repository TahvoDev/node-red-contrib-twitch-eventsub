{
  "name": "@tahvo/node-red-twitch-api",
  "version": "0.1.1",
  "description": "Connect to Twitch API",
  "main": "index.js",
  "homepage": "https://github.com/TahvoDev/node-red-contrib-twitch-api",
  "repository": "https://github.com/TahvoDev/node-red-contrib-twitch-api",
  "scripts": {
    "build": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\" && ttsc && npm run copy-static-assets",
    "copy-static-assets": "node scripts/copy-static-assets.js",
    "node-red-check": "node-red-dev validate",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "node-red",
    "twitch",
    "stream",
    "rewards",
    "bits",
    "cheer",
    "follow",
    "subscribe"
  ],
  "author": {
    "email": "tahvobiz@pm.me",
    "name": "Tahvo",
    "url": "https://tahvo.tech"
  },
  "license": "MIT",
  "dependencies": {
    "@twurple/api": "^7.3.0",
    "@twurple/auth": "^7.3.0",
    "@twurple/common": "^7.3.0",
    "@twurple/eventsub-ws": "^7.3.0"
  },
  "node-red": {
    "version": ">=2.0.0",
    "nodes": {
      "twitch-api-config": "dist/twitch/twitch-api-config.js",
      "twitch-eventsub-channel-redemption-add": "dist/twitch/eventsub/twitch-eventsub-channel-redemption-add.js",
      "twitch-eventsub-channel-stream-online": "dist/twitch/eventsub/twitch-eventsub-channel-stream-online.js",
      "twitch-eventsub-channel-stream-offline": "dist/twitch/eventsub/twitch-eventsub-channel-stream-offline.js",
      "twitch-eventsub-channel-subscription": "dist/twitch/eventsub/twitch-eventsub-channel-subscription.js",
      "twitch-eventsub-channel-cheer": "dist/twitch/eventsub/twitch-eventsub-channel-cheer.js",
      "twitch-eventsub-channel-raid-from": "dist/twitch/eventsub/twitch-eventsub-channel-raid-from.js",
