      "twitch-eventsub-channel-follow": "dist/twitch/eventsub/twitch-eventsub-channel-follow.js",
      "twitch-eventsub-channel-subscription-gift": "dist/twitch/eventsub/twitch-eventsub-channel-subscription-gift.js",
      "twitch-eventsub-channel-chat-message": "dist/twitch/eventsub/twitch-eventsub-channel-chat-message.js",
      "twitch-helix-get-users": "dist/twitch/helix/twitch-helix-get-users.js"
